<?xm version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.ui.view" id="spaceship_view_form">
            <field name="name">space.ship.form</field>
            <field name="model">space.ship</field>
            <field name="arch" type="xml">
                <form string="SpaceShip Form">
                    <sheet>
                        <h1><field name="name"/></h1>
                        <group>
                            <group>
                                <field name="active"/>
                            </group>
                            <group>
                                <field name="prototipos"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Description1">
                                <field name="description"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="spaceship_view_list">
            <field name="name">space.ship.list</field>
            <field name="model">space.ship</field>
            <field name="arch" type="xml">
                <tree string="SpaceShip List">
                    <field name="name"/>
                    <field name="description"/>
                    <field name="prototipos"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="spaceship_view_search">
            <field name="name">space.ship.search</field>
            <field name="model">space.ship</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="description"/>
                </search>
            </field>
        </record>
    </data>       
</odoo>