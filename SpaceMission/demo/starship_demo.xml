<odoo>
    <data noupdate="1">
        <record model="space.ship" id="starship_mk1">
            <field name="name">Starship Mk1</field>
            <field name="description">Starship Mk1 fue el primer prototipo de Starship</field>
            <field name="fuel_leo">10000</field>
            <field name="fuel_return">5000</field>
            <field name="diameter">1000</field>
            <field name="length">1000</field>
